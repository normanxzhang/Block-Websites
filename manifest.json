{
    "manifest_version": 2,
    "name": "BLOCK-WEBSITES",
    "version": "1.0",
  
    "description": "Adds a red border to all webpages matching mozilla.org.",
  
    "icons": {
      "48": "icons/E-Commerce-Icon-Set-icon.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["block.js"]
      }
    ]
  }
  